class Funnel:
    def __init__(self):
            self.leads = []
                    self.opportunities = []
                            self.customers = []

                                def add_lead(self, lead):
                                        self.leads.append(lead)

                                            def convert_to_opportunity(self, lead):
                                                    self.leads.remove(lead)
                                                            self.opportunities.append(lead)

                                                                def convert_to_customer(self, opportunity):
                                                                        self.opportunities.remove(opportunity)
                                                                                self.customers.append(opportunity)

                                                                                    def get_leads(self):
                                                                                            return self.leads

                                                                                                def get_opportunities(self):
                                                                                                        return self.opportunities

                                                                                                            def get_customers(self):
                                                                                                                    return self.customers


                                                                                                                    # Example usage
                                                                                                                    funnel = Funnel()

                                                                                                                    # Add some leads
                                                                                                                    funnel.add_lead("Lead 1")
                                                                                                                    funnel.add_lead("Lead 2")

                                                                                                                    # Convert a lead to an opportunity
                                                                                                                    leads = funnel.get_leads()
                                                                                                                    funnel.convert_to_opportunity(leads[0])

                                                                                                                    # Convert an opportunity to a customer
                                                                                                                    opportunities = funnel.get_opportunities()
                                                                                                                    funnel.convert_to_customer(opportunities[0])

                                                                                                                    # Get the current state of the funnel
                                                                                                                    leads = funnel.get_leads()
                                                                                                                    opportunities = funnel.get_opportunities()
                                                                                                                    customers = funnel.get_customers()

                                                                                                                    print("Leads:", leads)
                                                                                                                    print("Opportunities:", opportunities)
                                                                                                                    print("Customers:", customers)
                                                                                                  class Funnel {
                                                                                                      constructor() {
                                                                                                          this.leads = [];
                                                                                                              this.opportunities = [];
                                                                                                                  this.customers = [];
                                                                                                                    }

                                                                                                                      addLead(lead) {
                                                                                                                          this.leads.push(lead);
                                                                                                                            }

                                                                                                                              convertToOpportunity(lead) {
                                                                                                                                  const leadIndex = this.leads.indexOf(lead);
                                                                                                                                      if (leadIndex !== -1) {
                                                                                                                                            this.leads.splice(leadIndex, 1);
                                                                                                                                                  this.opportunities.push(lead);
                                                                                                                                                      }
                                                                                                                                                        }

                                                                                                                                                          convertToCustomer(opportunity) {
                                                                                                                                                              const opportunityIndex = this.opportunities.indexOf(opportunity);
                                                                                                                                                                  if (opportunityIndex !== -1) {
                                                                                                                                                                        this.opportunities.splice(opportunityIndex, 1);
                                                                                                                                                                              this.customers.push(opportunity);
                                                                                                                                                                                  }
                                                                                                                                                                                    }

                                                                                                                                                                                      getLeads() {
                                                                                                                                                                                          return this.leads;
                                                                                                                                                                                            }

                                                                                                                                                                                              getOpportunities() {
                                                                                                                                                                                                  return this.opportunities;
                                                                                                                                                                                                    }

                                                                                                                                                                                                      getCustomers() {
                                                                                                                                                                                                          return this.customers;
                                                                                                                                                                                                            }
                                                                                                                                                                                                            }

                                                                                                                                                                                                            // Example usage
                                                                                                                                                                                                            const funnel = new Funnel();

                                                                                                                                                                                                            // Add some leads
                                                                                                                                                                                                            funnel.addLead("Lead 1");
                                                                                                                                                                                                            funnel.addLead("Lead 2");

                                                                                                                                                                                                            // Convert a lead to an opportunity
                                                                                                                                                                                                            const leads = funnel.getLeads();
                                                                                                                                                                                                            funnel.convertToOpportunity(leads[0]);

                                                                                                                                                                                                            // Convert an opportunity to a customer
                                                                                                                                                                                                            const opportunities = funnel.getOpportunities();
                                                                                                                                                                                                            funnel.convertToCustomer(opportunities[0]);

                                                                                                                                                                                                            // Get the current state of the funnel
                                                                                                                                                                                                            const leads = funnel.getLeads();
                                                                                                                                                                                                            const opportunities = funnel.getOpportunities();
                                                                                                                                                                                                            const customers = funnel.getCustomers();

                                                                                                                                                                                                            console.log("Leads:", leads);
                                                                                                                                                                                                            console.log("Opportunities:", opportunities);
                                                                                                                                                                                                            console.log("Customers:", customers);
                                                                                            
                                                                                                  }                  